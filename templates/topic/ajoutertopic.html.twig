{% extends 'backOffice.html.twig' %}

{% block body %}
	<div class="form-container">
		<h1>AJOUTER Topic</h1>
		<form method="post" enctype="multipart/form-data">
			{{ form_start(formulairetopic) }}
			<div class="form-group">
				{{ form_row(formulairetopic.Nom) }}
			</div>
			<div class="form-group">
				{{ form_row(formulairetopic.Subject) }}
			</div>
			<div class="form-group">
				{{ form_row(formulairetopic.Image) }}
			</div>
			<div class="form-group">
				<button type="submit" class="btn btn-primary">Submit</button>
			</div>
			{{ form_end(formulairetopic) }}
		</form>
	</div>
{% endblock %}
