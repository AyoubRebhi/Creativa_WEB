{% extends 'base.html.twig' %}

{% block title %}Projet{% endblock %}

{% block body %}
    <h1>Projet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdProjet</th>
                <td>{{ projet.idProjet }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ projet.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ projet.description }}</td>
            </tr>
            <tr>
                <th>Media</th>
                <td>{{ projet.media }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ projet.prix }}</td>
            </tr>
            <tr>
                <th>Isvisible</th>
                <td>{{ projet.isvisible ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ projet.createdAt ? projet.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ projet.updatedAt ? projet.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_projet_index') }}">back to list</a>

    <a href="{{ path('app_projet_edit', {'idProjet': projet.idProjet}) }}">edit</a>

    {{ include('projet/_delete_form.html.twig') }}
{% endblock %}
