{% extends 'base.html.twig' %}

{% block title %}
	Projet
{% endblock %}

{% block body %}
	<div class="row">
		{% for projet in projets %}
			<div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
				<div class="property-item rounded overflow-hidden">
					<div class="position-relative overflow-hidden">
						<a href=""><img class="img-fluid" src="{{ asset('path_to_your_project_image/' ~ projet.media) }}" alt=""></a>
						<div class="bg-white rounded-top text-primary position-absolute start-0 bottom-0 mx-4 pt-1 px-3">{{ projet.categorie }}</div>
					</div>
					<div class="p-4 pb-0">
						<h5 class="text-primary mb-3">{{ projet.prix }}</h5>
						<a class="d-block h5 mb-2" href="">{{ projet.titre }}</a>
					</div>
					<div class="d-flex border-top">
						<small class="flex-fill text-center border-end py-2">
							<i class="fa fa-ruler-combined text-primary me-2"></i>

							10 likes</small>
					</div>
				</div>
			</div>
		{% endfor %}
	</div>
{% endblock %}
